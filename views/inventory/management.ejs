<% if (title) { %>
    <h1>
        <%= title %>
    </h1>
    <% } else { res.redirect('/') } %>

        <!-- Message -->
        <% if (errors) { %>
            <ul class="notice">
                <% errors.array().forEach(error=> { %>
                    <li>
                        <%= error.msg %>
                    </li>
                    <% }) %>
            </ul>
            <% } %>

                <!-- Links -->
                <a class="management-link" href="/inventory/add-classification">Add New Classification</a>
                <a class="management-link" href="/inventory/add-vehicle">Add New Vehicle</a>